<%- if controller_name != 'sessions' %>
  <%= link_to "Log in", new_session_path(resource_name),class:"mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect" %><br />
<% end -%>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to "Sign in with #{provider.to_s.titleize}", omniauth_authorize_path(resource_name, provider) ,class:"mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect"%><br />
  <% end -%>
<% end -%>
